<section class="main-content">
    <div class="main-content-header">
        <span class="mobile-menu-icon">
            <i class="bi bi-list"></i>
        </span>
        <div class="main-content-header-top">
            <div class="admin-info">
                <img src="/images/admin-user-image.png" alt="">
                <h6>Admin</h6>
            </div>
            <a href="/admin/log-out" class="logout-btn">Logout</a>
        </div>

        <div class="main-content-header-bottom">
            <h4>Subscription Overed User</h4>
            <div class="main-content-header-bottom-button">
                <a href="/admin/delete-all-subscription-overed-users" class="btn btn-primary">Remove All
                    Subscription</a>

            </div>
        </div>
    </div>
    <div class="user-content py-5">
        <div class="message-table">
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">User Name</th>
                            <th scope="col">Mobile Number</th>
                            <th scope="col">Email</th>
                            <th scope="col">Subscription Created</th>
                            <th scope="col">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(let i=0;i<subscriptionOveredUsers.length;i++) { %>
                        <tr>
                            <th scope="row">
                                <%= subscriptionOveredUsers[i].user_name %>
                            </th>
                            <td>
                                <%= subscriptionOveredUsers[i].user_mobile_number %>
                            </td>
                            <td>
                                <%= subscriptionOveredUsers[i].user_email %>
                            </td>
                            <td>
                                <p class="multiple-line-message">
                                    <%= new Date(subscriptionOveredUsers[i].created_at).toDateString('en-US') %>
                                </p>
                            </td>
                            <td>
                                <div class="table-action-btn">
                                    <a href="/admin/delete-subscription-overed-user?id=<%= subscriptionOveredUsers[i].user_id %>"
                                        data-bs-toggle="modal"
                                        data-bs-target="/admin/delete-subscription-overed-user?id=<%= subscriptionOveredUsers[i].user_id %>">
                                        <i class="bi bi-trash-fill"></i>
                                    </a>
                                </div>
                            </td>
                        </tr>
                        <div class="modal fade"
                            id="deleteUserReport<%= subscriptionOveredUsers[i].subscriptionOveredUsers_id %>"
                            tabindex="-1"
                            aria-labelledby="deleteUserReport<%= subscriptionOveredUsers[i].subscriptionOveredUsers_id %>Label"
                            aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h1 class="modal-title fs-5" id="exampleModalLabel">Delete User Report</h1>
                                        <button type="button" class="btn-close" data-bs-dismiss="modal"
                                            aria-label="Close"></button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="add-banner-form">
                                            <h3>Are you want to delete?</h3>
                                        </div>
                                        <a href="/admin/delete-user-report/<%= subscriptionOveredUsers[i].subscriptionOveredUsers_id %>"
                                            class="btn btn-danger">Delete</a>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-secondary"
                                            data-bs-dismiss="modal">Close</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>
<script>
    document.addEventListener("DOMContentLoaded", function () {
        const activeItem = document.querySelector(".active");
        if (activeItem) {
            activeItem.scrollIntoView(false);
        }
    });
